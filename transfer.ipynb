   "source": [
    "import glob\n",
    "import pandas as pd\n",
    "catfs = glob.glob(\"train/train/cat.*.jpg\")\n",
    "catt = [0] * len(catfs)\n",
    "dogfs = glob.glob(\"train/train/dog.*.jpg\")\n",
    "dogt = [1] * len(dogfs)\n",
    "df = pd.DataFrame({\n",
    "    \"path\":catfs + dogfs,\n",
    "    \"ans\":catt + dogt\n",
    "})\n",
    "df"
   ]
